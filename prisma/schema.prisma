// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Student {
  id        String @id @default(uuid())
  name      String
  cityName String
  schoolName String
  className String
  grade String

  @@map("students")
}
